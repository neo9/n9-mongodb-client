{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "strict": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "declaration": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "sourceMap": true
  },
  "exclude": ["node_modules", "dist"]
}
